{
  "name": "helen",
  "version": "1.0.0",
  "description": "Helen of Troy was a woman who was said to be the most beautiful woman in ancient Greece. She now has the honour of being the name of our website's git repo.",
  "main": "index.js",
  "scripts": {
    "build-css": "npx tailwindcss -i ./assets/tailwind.css -o ./assets/css/tailwind.css",
    "watch-css": "npx tailwindcss -i ./assets/tailwind.css -o ./assets/css/tailwind.css --watch",
    "build": "npm run build-css && hugo --minify",
    "dev": "npm run watch-css & hugo server --buildDrafts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeMonkeyCybersecurity/helen.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/CodeMonkeyCybersecurity/helen/issues"
  },
  "homepage": "https://github.com/CodeMonkeyCybersecurity/helen#readme",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.17"
  }
}
